<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if (orders.length <= 0){%>
                <h1>Nothing there!</h1>
            <%} else {%>
                <ul>
                    <% orders.forEach(order => { %>
                        <li>
                            <h1># <%= order.id %> </h1>
                        </li>
                        <ul>
                            <% order.products.forEach(products =>{ %>
                                <li> <%= products.title%> <%= products.orderItem.quantity %> %> </li>
                            <%})%>
                        </ul>
                    <%})%>
                </ul>
            <%}%>
        </main>
        <%- include('../includes/end.ejs') %>